{% block master_block %}
    <!DOCTYPE html>
    <html>

    {% block head %}
        <head>
            {% include "base/head.html.twig" %}
        </head>
    {% endblock %}

    {% block body %}
        <body>

        {% block header %}
            {% include "base/header.html.twig" %}
        {% endblock %}

        {% block content %}
            <div id="content">
                {% block page_wrap %}
                    {% include "base/content.html.twig" %}
                {% endblock %}
            </div>
        {% endblock %}

        {% block footer %}
            <div id="footer">
                {% include "base/footer.html.twig" %}
            </div>
        {% endblock %}

        </body>
    {% endblock %}

    </html>
{% endblock %}