{% for variable in variables %}
    <tr>

        <td>{{ variable.namespace }}</td>
        <td>{{ variable.name }}</td>
        <td>{{ variable.type }}</td>
        <td>{{ variable.value }}</td>
        <td>
            <span class="variable-action-container">
                {% if variable.editable %}
                    <div class="btn btn-warning">B</div>
                    <div class="btn btn-danger">L</div>
                {% endif %}
            </span>
        </td>

    </tr>
{% endfor %}


