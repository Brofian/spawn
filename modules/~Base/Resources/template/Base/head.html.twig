{% block document_head %}
    <head>

        {% block document_title %}
            <title>Webu Anwendung!</title>
        {% endblock %}

        {% block document_charset %}
            <meta charset="UTF-8" />
        {% endblock %}

        {% block document_viewport %}
            <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
        {% endblock %}

        {% block document_javascript %}
            <script src="{{ assetPath(namespace) }}/js/index.js{% if environment == "dev" %}?i={{ random() }}{% endif %}" async></script>
        {% endblock %}

        {% block document_css %}
            {# preload fonts before creating html #}
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-Bold" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-BoldItalic" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-Italic" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-Light" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-LightItalic" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-Regular" as="font" type="font/ttf" crossorigin/>


            <link rel="stylesheet" href="{{ assetPath(namespace) }}/css/all.min.css">
        {% endblock %}

        {% block document_default_css %}
            {% set defaultNamespace = 'global'|hash %}
            {% if namespace != defaultNamespace %}
                <link rel="stylesheet" href="{{ assetPath(defaultNamespace) }}/css/all.min.css">
            {% endif %}
        {% endblock %}


    </head>
{% endblock %}