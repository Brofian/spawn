{% block document_head %}
    <head>

        {% block document_title %}
            <title>Webu Anwendung!</title>
        {% endblock %}

        {% block document_meta %}
        {% endblock %}

        {% block document_charset %}
            <meta charset="UTF-8" />
        {% endblock %}

        {% block document_favicon %}
            <link rel="icon" type="image/svg+xml" href="{{ assetPath(namespace) }}/assets/icons/favicon.svg">
        {% endblock %}

        {% block document_viewport %}
            <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
        {% endblock %}

        {% block document_javascript %}
            <script src="{{ assetPath(namespace) }}/js/index.js{% if environment == "dev" %}?i={{ random() }}{% endif %}" async></script>
        {% endblock %}

        {% block document_css %}
            {# preload fonts before creating html #}
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-Bold.ttf" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-BoldItalic.ttf" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-Italic.ttf" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-Light.ttf" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-LightItalic.ttf" as="font" type="font/ttf" crossorigin/>
            <link rel="preload" href="{{ assetPath(namespace) }}/assets/fonts/poppins/Poppins-Regular.ttf" as="font" type="font/ttf" crossorigin/>


            <link rel="stylesheet" href="{{ assetPath(namespace) }}/css/all.min.css">
        {% endblock %}

    </head>
{% endblock %}